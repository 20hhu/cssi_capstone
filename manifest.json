{ 
    "manifest_version": 2,
    "name": "CSSI extension",
    "version": "0.1",
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["libraries/p5.js", "script.js"]
    }],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_icon": "cat.png",
        "default_popup": "popup_test/index.html",
        "default_title": "popup description here"
    }
}