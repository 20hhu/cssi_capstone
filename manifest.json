{ 
    "manifest_version": 2,
    "name": "CSSI extension",
    "version": "0.1",
    
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["libraries/p5.js", "libraries/ml5.min.js", "libraries/p5.dom.min.js", "script.js", "background.js"]
    }],
    "background": {
        "page": "background.html"
    },
    "browser_action": {
        "default_icon": "cat.png",
        "default_popup": "popup_test/index.html",
        "default_title": "popup description here"
    },
    "permissions": [
        "activeTab",
        "storage",
        "unlimitedStorage"
    ],
    "web_accessible_resources": [
        "song.mp3"
    ]
}