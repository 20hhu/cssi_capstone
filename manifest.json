{ 
    "manifest_version": 2,
    "name": "CSSI extension",
    "version": "0.1",
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["libraries/p5.js", "libraries/ml5.min.js", "libraries/p5.dom.min.js", "script.js"]
    }],
    "background": {
        "scripts": ["libraries/p5.js", "background.js"],
        "persistent": false
    },
    "browser_action": {
        "default_icon": "cat.png",
        "default_popup": "popup_test/index.html",
        "default_title": "popup description here"
    },
    "permissions": ["storage"],
    "web_accessible_resources": [
        "stop.mp3"
    ]
}